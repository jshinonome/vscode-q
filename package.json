{
	"name": "vscode-q",
	"displayName": "kdb+/q",
	"description": "q language support for Visual Studio Code",
	"version": "1.1.0",
	"homepage": "https://github.com/jshinonome/vscode-q",
	"publisher": "jshinonome",
	"license": "MIT",
	"engines": {
		"vscode": "^1.45.1"
	},
	"categories": [
		"Programming Languages",
		"Snippets",
		"Formatters"
	],
	"maintainers": [
		"Jo Shinonome <jshinonome@gmail.com>"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/jshinonome/vscode-q.git"
	},
	"keywords": [
		"kdb+",
		"kdb",
		"q"
	],
	"icon": "icon.png",
	"main": "./out/extension",
	"activationEvents": [
		"*"
	],
	"scripts": {
		"vscode:package": "vsce package",
		"vscode:publish": "vsce publish",
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc --watch -p ./"
	},
	"contributes": {
		"languages": [
			{
				"id": "q",
				"aliases": [
					"q",
					"kdb+",
					"kdb"
				],
				"extensions": [
					".q"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "q",
				"scopeName": "source.q",
				"path": "./syntaxes/q.tmLanguage.json"
			}
		],
		"snippets": [
			{
				"language": "q",
				"path": "./snippets/q.json"
			}
		],
		"commands": [
			{
				"command": "qext.connect",
				"title": "qext connect"
			},
			{
				"command": "qext.query",
				"title": "qext query"
			}

		]
	},
	"devDependencies": {
		"@types/long": "^4.0.1",
		"@types/node": "^14.0.13",
		"@types/vscode": "^1.45.1",
		"eslint": "^7.2.0",
		"node-q": "^2.5.0",
		"typescript": "^3.9.5",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"tslint": "^6.1.2"
	}
}
