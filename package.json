{
	"name": "vscode-q",
	"displayName": "kdb+/q",
	"description": "q language support for Visual Studio Code",
	"version": "1.1.0",
	"homepage": "https://github.com/jshinonome/vscode-q",
	"publisher": "jshinonome",
	"license": "MIT",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Programming Languages",
		"Snippets",
		"Formatters"
	],
	"maintainers": [
		"Jo Shinonome <jshinonome@gmail.com>"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/jshinonome/vscode-q.git"
	},
	"keywords": [
		"kdb+",
		"kdb",
		"q"
	],
	"icon": "icon.png",
	"main": "./out/extension",
	"activationEvents": [
		"onView:qservers",
		"onCommand:qservers.refreshEntry"
	],
	"scripts": {
		"vscode:package": "vsce package",
		"vscode:publish": "vsce publish",
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc --watch -p ./"
	},
	"contributes": {
		"languages": [
			{
				"id": "q",
				"aliases": [
					"q",
					"kdb+",
					"kdb"
				],
				"extensions": [
					".q"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "q",
				"scopeName": "source.q",
				"path": "./syntaxes/q.tmLanguage.json"
			}
		],
		"snippets": [
			{
				"language": "q",
				"path": "./snippets/q.json"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "q-server-explorer",
					"title": "q Server Explorer",
					"icon": "media/server-svgrepo-com.svg"
				}
			]
		},
		"views": {
			"q-server-explorer": [
				{
					"id": "qservers",
					"name": "q Servers",
					"icon": "media/storage-24px.svg",
					"contextualTitle": "q Servers"
				}
			]
		},
		"commands": [
			{
				"command": "qservers.refreshEntry",
				"title": "Refresh q Server List",
				"icon": {
					"light": "resources/dark/delete-24px.svg",
					"dark": "resources/light/delete-24px.svg"
				}
			},
			{
				"command": "qservers.addEntry",
				"title": "Add q Server",
				"icon": {
					"light": "resources/dark/add-24px.svg",
					"dark": "resources/light/add-24px.svg"
				}
			},
			{
				"command": "qservers.editEntry",
				"title": "Edit q Server",
				"icon": {
					"light": "resources/dark/edit-24px.svg",
					"dark": "resources/light/edit-24px.svg"
				}
			},
			{
				"command": "qservers.deleteEntry",
				"title": "Delete q Server",
				"icon": {
					"light": "resources/dark/delete-24px.svg",
					"dark": "resources/light/delete-24px.svg"
				}
			}
		],
		"menu": {
			"view/title": [
				{
					"command": "qservers.refreshEntry",
					"when": "view == qservers",
					"group": "navigation"
				},
				{
					"command": "qservers.addEntry",
					"when": "view == qservers",
					"group": "navigation"
				}
			],
			"view/item/context": [
				{
					"command": "qservers.editEntry",
					"when": "view == qservers && viewItem == qconn",
					"group": "inline"
				},
				{
					"command": "qservers.deleteEntry",
					"when": "view == qservers && viewItem == qconn",
					"group": "inline"
				}
			]
		}
	},
	"devDependencies": {
		"@types/long": "^4.0.1",
		"@types/node": "^14.0.13",
		"@types/vscode": "^1.46.0",
		"eslint": "^7.2.0",
		"node-q": "^2.5.0",
		"typescript": "^3.9.5",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"tslint": "^6.1.2"
	}
}
